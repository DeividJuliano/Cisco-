# cole o script que você editou (shift insert) enter
#exemplo de script

interface Vlan666
 ip address 192.168.66.2 255.255.255.0
 ip helper-address 172.16.9.20
 standby 66 ip 192.168.66.1
 standby 66 priority 200
 standby 66 preempt
!
exit
!
vlan 666
!
end
wr
